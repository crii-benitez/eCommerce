<h1>Productos</h1>
<div>
    <h2>Listado de Productos:</h2>
    <table class="table table-hover">
        <thead>
            <th>Id</th>
            <th>Categoria</th>
            <th>Nombre</th>
            <th>Descripcion</th>
            <th>Stock</th>
            <th>Precio</th>
            <th>Oferta</th>
        </thead>
        <tbody>
            {{#each productos}}
            <tr>
                <td>{{id}}</td>
                <td>{{id_categoria}}</td>
                <td>{{nombre}}</td>
                <td>{{descripcion}}</td>
                <td>{{stock}}</td>
                <td>{{precio}}</td>
                <td>{{oferta}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>

